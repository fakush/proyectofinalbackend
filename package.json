  "description": "Primera entrega del proyecto final; del curso Programaci√≥n Backend",
  "main": "index.js",
  "scripts": {
    "start": "node_modules/.bin/tsc && node ./dist/src/index.js --cluster",
    "start:args": "node ./dist/src/index.js --port=3000 --fbClientId=myId --fbClientSecret=mySecret",
    "start:cluster": "node ./dist/src/index.js --cluster=true",
    "start:forevercluster": "node_modules/.bin/forever -w start ./dist/src/index.js --cluster=true",
    "start:foreverfork": "node_modules/.bin/forever -w start ./dist/src/index.js",
    "stop:forever": "node_modules/.bin/forever stopall",
    "start:pm2": "node_modules/.bin/pm2 start ecosystem.config.js --watch",
    "stop:pm2": "node_modules/.bin/pm2 stop all",
    "build": "node_modules/.bin/tsc",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon",
    "make_env": "node ./scr/make-env.ts",
    "start:profilling": "node --prof dist/src/index.js",
    "start:inspect": "node --inspect dist/src/index.js",
    "start:0x": "0x dist/src/index.js",
    "start:clinic": "clinic doctor -- node dist/src/index.js"
  },
